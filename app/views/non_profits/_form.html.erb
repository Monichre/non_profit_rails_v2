<%= render "layouts/errors", :object => @non_profit %>
<%= form_for @non_profit do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= label_tag :category %>
  <%= text_field_tag :category, params[:category] %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.submit  %>
<% end %>
