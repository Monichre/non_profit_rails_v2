<h1>Browse our non profits</h1>

<% if @non_profits.any? %>
  <ul>
    <% @non_profits.each do |non_profit| %>
      <li><%= link_to non_profit.name, non_profit_path(non_profit) %></li>
    <% end %>
  </ul>
<% else %>
  <p>
    No non profits at this time
  </p>
<% end %>

<p>
  <%= link_to "Add a new non profit", new_non_profit_path %>
</p>
<p>
  <%= link_to "Browse by category", categories_path %>
</p>
